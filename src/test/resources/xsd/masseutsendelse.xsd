<?xml version="1.0" encoding="UTF-8"?>
<!--
Digipost - grensesnitt for avsendere
Masseutsendelse request
-->
<xsd:schema version="1.0"
    targetNamespace="http://www.digipost.no/xsd/avsender2_0"
    xmlns:dp="http://www.digipost.no/xsd/avsender2_0"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	jaxb:version="2.0"
	elementFormDefault="qualified">

	
    <!-- jobb-innstillinger -->
    <xsd:complexType name="jobb-innstillinger">
        <xsd:annotation>
            <xsd:documentation>
                Definerer parametre som gjelder hele jobben.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="avsender-id" type="xsd:long" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		                Avsenders unike kunde-id i Digipost.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="behandler-id" type="xsd:long" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		                Behandlers unike kunde-id i Digipost. Behandleren er den parten som sender jobben på vegne av avsenderen.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="jobb-id" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		                Avsenders unike id for denne masseutsendelse-jobben. Samme id kan ikke benyttes for ulike jobber.
		            </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:pattern value="[a-zA-Z0-9_.-]{1,100}"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="jobb-navn" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		                Vilkårlig navn/beskrivelse for jobben.
		            </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="80"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="auto-godkjenn-jobb" type="xsd:boolean" minOccurs="0" maxOccurs="1" default="false"/>
            <xsd:element name="klientinformasjon" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
						Navn og versjon på avsenderprogram, eventuelt plattform/OS.
					</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- mottaker-liste -->
    <xsd:complexType name="mottaker-liste">
        <xsd:annotation>
            <xsd:documentation>
            	Liste av mottakere.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="mottaker" type="dp:mottaker" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>
		            	En mottaker.
		            </xsd:documentation>
		            <xsd:appinfo>
		            	<jaxb:property name="mottakere"/>
		            </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- mottaker -->
    <xsd:complexType name="mottaker">
        <xsd:annotation>
            <xsd:documentation>
            	Definerer en Digipost-postmottaker.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="kunde-id" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Avsenders egne unike id for postmottakeren. Et brev kan ikke sendes til den samme kunde-id-en mer enn én gang.
		            	Denne vil benyttes i kvitteringsfilen for å angi resultat per mottaker.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:sequence>
                    <xsd:element name="navn" type="dp:navn" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
                                Mottakers navn.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="adresse" type="dp:adresse" minOccurs="1" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
				            	Mottakers adresse.
				            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="foedselsdato" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
				            	Mottakers fødselsdato. Kan øke sannsynligheten for sikker identifisering. Format : ddMMyy
				            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="telefonnummer" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
				            	Mottakers telefonnummer. Kan øke sannsynligheten for sikker identifisering. Fortrinnsvis mobilnummer.
				            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="epostadresse" type="xsd:string" minOccurs="0" maxOccurs="1">
                        <xsd:annotation>
                            <xsd:documentation>
				            	Mottakers epostadresse. Kan øke sannsynligheten for sikker identifisering.
				            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
                <xsd:element name="foedselsnummer" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>
			            	Gyldig norsk fødselsnummer eller D-nummer.
			            </xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="digipostadresse" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>
							Mottakerens Digipost-adresse.
						</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element name="person-alias" type="xsd:string" minOccurs="1" maxOccurs="1">
                    <xsd:annotation>
                        <xsd:documentation>
							Alias for en identifisert person som ikke har opprettet en Digipost-konto.
						</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- navn -->
    <xsd:complexType name="navn">
        <xsd:choice minOccurs="1" maxOccurs="1">
            <xsd:element name="navn-format1" type="dp:navn-format1" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="navn-format2" type="dp:navn-format2" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="navn-format3" type="dp:navn-format3" minOccurs="1" maxOccurs="1"/>
        </xsd:choice>
    </xsd:complexType>
    
    <!-- navn-format1 -->
    <xsd:complexType name="navn-format1">
        <xsd:choice>
            <xsd:element name="fullt-navn-fornavn-foerst" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
	                    Mottakers fulle navn på formen «Fornavn Mellomnavn Etternavn».
	                </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="fullt-navn-etternavn-foerst" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
	                    Mottakers fulle navn på formen «Etternavn Fornavn Mellomnavn».
	                </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>
    
    <!-- navn-format2 -->
    <xsd:complexType name="navn-format2">
        <xsd:sequence>
            <xsd:element name="fornavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                            Mottakers fornavn.
                        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="etternavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                            Mottakers etternavn.
                        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- navn-format3 -->
    <xsd:complexType name="navn-format3">
        <xsd:sequence>
            <xsd:element name="fornavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Mottakers fornavn.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="mellomnavn" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Mottakers mellomnavn.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="etternavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Mottakers etternavn.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- afp-navn -->
    <xsd:complexType name="afp-navn">
        <xsd:choice minOccurs="1" maxOccurs="1">
            <xsd:element name="afp-navn-format1" type="dp:afp-navn-format1" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="afp-navn-format2" type="dp:afp-navn-format2" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="afp-navn-format3" type="dp:afp-navn-format3" minOccurs="1" maxOccurs="1"/>
        </xsd:choice>
    </xsd:complexType>
    
    <!-- afp-navn-format1 -->
    <xsd:complexType name="afp-navn-format1">
        <xsd:choice>
            <xsd:element name="fullt-navn-fornavn-foerst" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
	                    Navn på TLE-felt som inneholder mottakers fulle navn på formen «Fornavn Mellomnavn Etternavn».
	                </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="fullt-navn-etternavn-foerst" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
	                    Navn på TLE-felt som inneholder mottakers fulle navn på formen «Etternavn Fornavn Mellomnavn».
	                </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>
    
    <!-- afp-navn-format2 -->
    <xsd:complexType name="afp-navn-format2">
        <xsd:sequence>
            <xsd:element name="fornavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                            Navn på TLE-felt som inneholder mottakers fornavn.
                        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="etternavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                            Navn på TLE-felt som inneholder mottakers etternavn.
                        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- afp-navn-format3 -->
    <xsd:complexType name="afp-navn-format3">
        <xsd:sequence>
            <xsd:element name="fornavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Navn på TLE-felt som inneholder mottakers fornavn.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="mellomnavn" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Navn på TLE-felt som inneholder mottakers mellomnavn.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="etternavn" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Navn på TLE-felt som inneholder mottakers etternavn.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- adresse -->
    <xsd:complexType name="adresse">
        <xsd:choice>
            <xsd:element name="adresse-format1" type="dp:adresse-format1" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="adresse-format2" type="dp:adresse-format2" minOccurs="1" maxOccurs="1"/>
        </xsd:choice>
    </xsd:complexType>
    
    <!-- adresse-format1 -->
    <xsd:complexType name="adresse-format1">
        <xsd:sequence>
            <xsd:element name="adresselinje1" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        For gateadresse vil dette være [Testgate 2b]
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresselinje2" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>

                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="postnummer" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="poststed" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="land" type="xsd:string" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- adresse-format2 -->
    <xsd:complexType name="adresse-format2">
        <xsd:sequence>
            <xsd:element name="adresse1" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        For gateadresse vil dette være: [Gatenavn].
                        For stedsadresse vil dette være : [Stedsnavn].
                        For postboksadresse vil dette være : Postboks  [postboksnummer] [sted].
                        For postboksadresse vil dette være : Serviceboks  [boksnummer].
                        For utenlandsadresse vil en benytte den kolonnen for å legge inn første adresselinje.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresse2" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        For gateadresse vil dette være: [Husnr].
                        Eventuell adresselinje 2 for utenlandsadresser.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresse3" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        For gateadresse vil dette være: [Bokstav].
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresse4" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        For gateadresse vil dette være: [Inngang]
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="postnummer" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="poststed" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="land" type="xsd:string" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- afp-adresse -->
    <xsd:complexType name="afp-adresse">
        <xsd:choice>
            <xsd:element name="afp-adresse-format1" type="dp:afp-adresse-format1" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="afp-adresse-format2" type="dp:afp-adresse-format2" minOccurs="1" maxOccurs="1"/>
        </xsd:choice>
    </xsd:complexType>
    
    <!-- afp-adresse-format1 -->
    <xsd:complexType name="afp-adresse-format1">
        <xsd:sequence>
            <xsd:element name="adresselinje1" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Navn på TLE-felt som inneholder gateadresse [Testgate 2b]
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresselinje2" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                        Navn på TLE-felt som inneholder postnummer og sted [0128 Oslo]
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="postnummer" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder postnummer.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="poststed" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder poststed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="land" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder land.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- afp-adresse-format2 -->
    <xsd:complexType name="afp-adresse-format2">
        <xsd:sequence>
            <xsd:element name="adresse1" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder adresseinformasjon 1.
                        For gateadresse vil dette være: [Gatenavn].
                        For stedsadresse vil dette være : [Stedsnavn].
                        For postboksadresse vil dette være : Postboks  [postboksnummer] [sted].
                        For postboksadresse vil dette være : Serviceboks  [boksnummer].
                        For utenlandsadresse vil en benytte den kolonnen for å legge inn første adresselinje.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresse2" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder adresseinformasjon 2.
                        For gateadresse vil dette være: [Husnr].
                        Eventuell adresselinje 2 for utenlandsadresser.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresse3" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder adresseinformasjon 3.
                        For gateadresse vil dette være: [Bokstav].
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="adresse4" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder adresseinformasjon 4.
                        For gateadresse vil dette være: [Inngang]
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="postnummer" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder postnummer.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="poststed" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder poststed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="land" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
                    	Navn på TLE-felt som inneholder land.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- jobb-info -->
    <xsd:complexType name="jobb-info">
        <xsd:sequence>
            <xsd:element name="ref-avsender-id" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Id for den Digipost-kontoen som er avsender for denne jobben.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ref-behandler-id" type="xsd:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Id for den Digipost-kontoen som sendte inn (behandlet) den jobben denne responsen gjelder.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ref-jobb-id" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Id på jobben som dette er resultat for.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="start-tid" type="xsd:dateTime" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Når jobben ble mottatt.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="slutt-tid" type="xsd:dateTime" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Når prosessering av jobben ble avsluttet.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- jobb-status -->
    <xsd:simpleType name="jobb-status">
        <xsd:annotation>
            <xsd:appinfo>
                <jaxb:typesafeEnumClass/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="SUKSESS">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="SUKSESS"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="FEILET">
                <xsd:annotation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="FEILET"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
    <!-- feilinformasjon -->
    <xsd:complexType name="feilinformasjon">
        <xsd:sequence>
            <xsd:element name="feilkode" type="dp:feilkode" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="melding" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
						Nærmere informasjon om hva som har gått galt.
					</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!-- feilkode -->
    <xsd:simpleType name="feilkode">
        <xsd:annotation>
            <xsd:appinfo>
                <jaxb:typesafeEnumClass/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="VALIDERING_FEILET">
                <xsd:annotation>
                    <xsd:documentation>Validering av XML-en feilet.</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="VALIDERING_FEILET"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ENGINE_FEIL">
                <xsd:annotation>
                    <xsd:documentation>En feil oppstod under prosesseringen av jobben.</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="ENGINE_FEIL"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="GENERELL_FEIL">
                <xsd:annotation>
                    <xsd:documentation>En uspesifisert feil oppstod.</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="GENERELL_FEIL"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
    <!--  mottaker-sammendrag -->
    <xsd:complexType name="mottaker-sammendrag">
        <xsd:sequence>
            <xsd:element name="total" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Totalt antall mottakere.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="digipost" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Totalt antall Digipost brukere identifisert (og som fikk tilsendt brev).
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="digipost-vask" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Antall Digipost brukere som ble identifisert gjennom adressevask.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="identifisert" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Totalt antall identifiserte brukere som ikke var Digipost-brukere.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="identifisert-vask" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Totalt antall identifiserte brukere som ble identifisert gjennom adressevask og som ikke er Digipost-brukere.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="uidentifisert" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Totalt antall ukjente brukere.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="uidentifisert-flere-treff" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Antall usikre treff (tilfeller hvor det fantes mer enn ett treff i Digiposts register for gitt mottakerinformasjon).
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ugyldig" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Totalt antall mottakere hvor noe av den oppgitte informasjonen var ugyldig.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <!--  print-sammendrag -->
    <xsd:complexType name="print-sammendrag">
        <xsd:sequence>
            <xsd:element name="sendt-til-print" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
						Antall mottakere som ble sendt til print.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ugyldig" type="xsd:int" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>
		            	Antall mottakere som ikke ble sendt til print fordi PDF-en eller printadressen var ugyldig.
		            </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<!--  mottaker-resultat-liste -->
    <xsd:complexType name="mottaker-resultat-liste">
        <xsd:annotation>
            <xsd:documentation>
            	Liste av mottakerresultater.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
        	<xsd:element name="resultat" type="dp:mottaker-resultat" minOccurs="0" maxOccurs="unbounded">
            	<xsd:annotation>
	                <xsd:appinfo>
		            	<jaxb:property name="resultater"/>
		            </xsd:appinfo>
            	</xsd:annotation>
        	</xsd:element>
        </xsd:sequence>
    </xsd:complexType>

	<!-- mottaker-resultat -->   
    <xsd:complexType name="mottaker-resultat">
        <xsd:sequence>
            <xsd:element name="kunde-id" type="xsd:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>Avsenders unike id for mottaker</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="status" type="dp:mottaker-status-kode" minOccurs="1" maxOccurs="1"/>
            <xsd:choice minOccurs="0" maxOccurs="1">
	            <xsd:element name="digipostadresse" type="xsd:string" minOccurs="1" maxOccurs="1">
	                <xsd:annotation>
	                    <xsd:documentation>Mottakerens Digipost-adresse. Kan brukes for å identifisere mottakere i fremtidige jobber.</xsd:documentation>
	                </xsd:annotation>
	            </xsd:element>
	            <xsd:element name="person-alias" type="xsd:string" minOccurs="1" maxOccurs="1">
	                <xsd:annotation>
	                    <xsd:documentation>Mottakerens alias hos Digipost. Brukes for personer som blir identifisert, men som ikke er Digipost-brukere.</xsd:documentation>
	                </xsd:annotation>
	            </xsd:element>
	            <xsd:element name="uidentifisert-grunn" type="dp:uidentifisert-kode" minOccurs="1" maxOccurs="unbounded"/>
	            <xsd:element name="ugyldig-grunn" type="dp:ugyldig-kode" minOccurs="1" maxOccurs="unbounded"/>
            </xsd:choice>
        	<xsd:element name="vasket" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

	<!-- mottaker-status-kode -->
    <xsd:simpleType name="mottaker-status-kode">
    	<xsd:restriction base="xsd:string">
            <xsd:enumeration value="DIGIPOST">
                <xsd:annotation>
                    <xsd:documentation>
                    	Mottakeren er en aktiv Digipost-bruker (har Digipost-adresse).
                    	Bruk returnert Digipost-adresse til å sende brev og sjekke status i fremtidige jobber.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="DIGIPOST"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="IDENTIFISERT">
                <xsd:annotation>
                    <xsd:documentation>
                    	Mottakeren er identifisert (har personalias), men er ikke p.t. en aktiv bruker.
                    	Bruk returnert personalias til å sjekke status i fremtidige jobber.
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="IDENTIFISERT"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UIDENTIFISERT">
                <xsd:annotation>
                    <xsd:documentation>
                    	Mottakeren kunne ikke identifiseres vha. oppgitt informasjon (sjekk navn/adressekvalitet).
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UIDENTIFISERT"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UGYLDIG">
                <xsd:annotation>
                    <xsd:documentation>
                    	Mottakeren kunne ikke identifiseres pga. ugyldig oppgitt informasjon (sjekk navn/adressekvalitet).
                    </xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UGYLDIG"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

	<!-- feil-liste : generell liste av feil -->
    <xsd:complexType name="feil-liste">
		<xsd:sequence>
		    <xsd:element name="feil" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
    </xsd:complexType>
    
    <!-- uidentifisert-kode -->
    <xsd:simpleType name="uidentifisert-kode">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="FLERE_TREFF">
                <xsd:annotation>
                    <xsd:documentation>Mottakeren kunne ikke identifiseres entydig</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="FLERE_TREFF"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="MANGELFULL_ADRESSE">
                <xsd:annotation>
                    <xsd:documentation>Mottakeren kunne ikke identifiseres på grunn av mangelfull adresse.</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="MANGELFULL_ADRESSE"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    
    <!-- ugyldig kode -->
    <xsd:simpleType name="ugyldig-kode">
        <xsd:annotation>
            <xsd:appinfo>
                <jaxb:typesafeEnumClass/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="UGYLDIG_KUNDEID">
                <xsd:annotation>
                    <xsd:documentation>Ugyldig kunde-id</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UGYLDIG_KUNDEID"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UGYLDIG_FOEDSELSNUMMER">
                <xsd:annotation>
                    <xsd:documentation>Ugyldig fødselsnummer</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UGYLDIG_FOEDSELSNUMMER"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UGYLDIG_FOEDSELSDATO">
                <xsd:annotation>
                    <xsd:documentation>Ugyldig fødselsdato</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UGYLDIG_FOEDSELSDATO"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UGYLDIG_DIGIPOSTADRESSE">
                <xsd:annotation>
                    <xsd:documentation>Ugyldig Digipost-adresse</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UGYLDIG_DIGIPOSTADRESSE"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UGYLDIG_NAVN_ADRESSE">
                <xsd:annotation>
                    <xsd:documentation>Ugyldig navn eller adresse</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UGYLDIG_NAVN_ADRESSE"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="UGYLDIG_POSTNUMMER">
                <xsd:annotation>
                    <xsd:documentation>Ugyldig postnummer</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="UGYLDIG_POSTNUMMER"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="USPESIFISERT">
                <xsd:annotation>
                    <xsd:documentation>Uspesifisert feil</xsd:documentation>
                    <xsd:appinfo>
                        <jaxb:typesafeEnumMember name="USPESIFISERT"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>


	<xsd:element name="masseutsendelse">
        <xsd:annotation>
            <xsd:documentation>
                Brukes til å foreta en masseutsendelse av brev i Digipost.
            </xsd:documentation>
        </xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="jobb-innstillinger" type="dp:jobb-innstillinger" minOccurs="1" maxOccurs="1" />
				<xsd:choice minOccurs="1" maxOccurs="1">
					<xsd:element name="standard-distribusjon" type="dp:standard-distribusjon" minOccurs="1" maxOccurs="1" />
					<xsd:element name="afp-distribusjon" type="dp:afp-distribusjon" minOccurs="1" maxOccurs="1" />
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="standard-distribusjon">
		<xsd:sequence>
			<xsd:element name="globale-brev-innstillinger" type="dp:brev-innstillinger" minOccurs="0" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		            	Innstillinger som skal gjelde dersom brevet selv ikke definerer sine egne innstillinger (default brev-innstillinger). Dersom innstillinger er angitt på dokumentnivå, vil disse overstyre de globale innstillingene.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:element name="globale-dokument-innstillinger" type="dp:dokument-innstillinger" minOccurs="0" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		            	Innstillinger som skal gjelde dersom dokumentet selv ikke definerer sine egne innstillinger (default dokument-innstillinger). Dersom innstillinger er angitt på dokumentnivå, vil disse overstyre de globale innstillingene.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:element name="post" type="dp:dokument-liste" minOccurs="1" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		            	Dokumenter som skal sendes i denne jobben.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:element name="forsendelser" type="dp:brev-liste" minOccurs="1" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		           		Liste over brev som skal sendes i denne jobben.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="afp-distribusjon">
		<xsd:annotation>
		    <xsd:documentation>
		   		Innstillinger for å sende brev fra en AFP-fil.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="afp-fil" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
				    <xsd:documentation>
				   		Path til AFP-fil, relativ til zip-rot.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="globale-dokument-innstillinger" type="dp:dokument-innstillinger" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
				    <xsd:documentation>
				   		Innstillinger som skal gjelde alle dokumentene i AFP-filen.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="mottaker-identifikasjon" type="dp:afp-mottaker-identifikasjon" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
				    <xsd:documentation>
				   		Beskriver hvordan mottakere skal identifiseres fra TLE-felter i AFP-filen.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
 			<xsd:element name="antall-dokumenter" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
				    <xsd:documentation>
				   		Antall dokumenter AFP-filen skal inneholde. Brukes til å validere at det riktige antallet PDF-filer er blitt generert.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="brev-innstillinger">
        <xsd:sequence>
			<xsd:element name="emne" minOccurs="1" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
	    	        	Emnet for brevet, slik det skal vises i Digipost.
	        	    </xsd:documentation>
		        </xsd:annotation>
		        <xsd:simpleType>
		        	<xsd:restriction base="xsd:string">
		        		<xsd:maxLength value="100"/>
		        	</xsd:restriction>
		        </xsd:simpleType>
			</xsd:element>
        </xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="dokument-innstillinger">
        <xsd:sequence>
			<xsd:element name="emne" minOccurs="1" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
	    	        	Emnet for dokumentet, slik det skal vises i Digipost.
	        	    </xsd:documentation>
		        </xsd:annotation>
		        <xsd:simpleType>
		        	<xsd:restriction base="xsd:string">
		        		<xsd:maxLength value="100"/>
		        	</xsd:restriction>
		        </xsd:simpleType>
			</xsd:element>
			<xsd:element name="sms-varsling" type="dp:sms-varsling" minOccurs="1" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		                Angir om mottaker skal varsles på SMS (prisbelagt tilleggstjeneste). 
		                Sms sendes kun dersom dokumentet er uåpnet på angitt varslingstidspunkt.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:element name="autentiseringsnivaa" type="dp:autentiseringsnivaa" minOccurs="0" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		                Angir autentiseringsnivå for dokumentet. Standardvalg er PASSORD. To-faktor er en prisbelagt tilleggstjeneste.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:element name="sensitivitetsnivaa" type="dp:sensitivitetsnivaa" minOccurs="0" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		                Angir sensitivitetsnivå for dokumentet. Standardvalg er NORMAL.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
        </xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="sms-varsling">
        <xsd:sequence>
        	<xsd:element name="tidspunkt" type="xsd:dateTime" minOccurs="0" maxOccurs="unbounded">
		        <xsd:annotation>
		            <xsd:documentation>
		                Angir et eller flere tispunkt for når SMS skal sendes ut dersom brevet fortsatt er ulest.
		                Vi sender kun ut SMS fra kl 9 - kl 20. Eventuelle tidspunkt som faller utenfor dette intervallet vil bli levert rundt kl 9 neste morgen.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:element name="etter-timer" type="dp:etter-timer" minOccurs="0" maxOccurs="unbounded">
		        <xsd:annotation>
		            <xsd:documentation>
		                Angir et eller flere tispunkt for når SMS skal sendes ut relativt til tidspunktet da brevet blir levert i postkassen.
		                Vi sender kun ut SMS fra kl 9 - kl 20. Eventuelle tidspunkt som faller utenfor dette intervallet vil bli levert rundt kl 9 neste morgen.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
        </xsd:sequence>
	</xsd:complexType>
	
	  <xsd:simpleType name="etter-timer"> 
	    <xsd:restriction base="xsd:int">
	      <xsd:minInclusive value="0"/>
	    </xsd:restriction>
	  </xsd:simpleType>	

	<xsd:complexType name="dokument-liste">
        <xsd:annotation>
            <xsd:documentation>
            	Liste av dokumenter.
            </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence minOccurs="1" maxOccurs="1">
       		<xsd:element name="dokument" type="dp:dokument" minOccurs="1" maxOccurs="unbounded">
            	<xsd:annotation>
	                <xsd:appinfo>
		            	<jaxb:property name="dokumenter"/>
		            </xsd:appinfo>
            	</xsd:annotation>
       		</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="dokument-ref-liste">
        <xsd:annotation>
            <xsd:documentation>
            	Liste av dokumenter (referanser).
            </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence minOccurs="1" maxOccurs="1">
			<xsd:element name="dokument" minOccurs="1" maxOccurs="99" type="xsd:IDREF" >
				<xsd:annotation>
		            <xsd:documentation>
		            	Id på hoveddokumentet som skal sendes til denne mottakeren.
		            	Merk: Id må begynne med en bokstav (f.eks. "D00001")
		            </xsd:documentation>
					<xsd:appinfo>
						<jaxb:property>
							<jaxb:baseType name="XmlDokument" />
						</jaxb:property>
					</xsd:appinfo>
				</xsd:annotation>
       		</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="brev-liste">
        <xsd:annotation>
            <xsd:documentation>
            	Liste av forsendelser.
            </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="brev" type="dp:brev" minOccurs="1" maxOccurs="unbounded">
		        <xsd:annotation>
		            <xsd:documentation>
		            	En forsendelse ("konvolutt").
		            </xsd:documentation>
	                <xsd:appinfo>
		            	<jaxb:property name="forsendelser"/>
		            </xsd:appinfo>
		        </xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="dokument">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:ID">
		        <xsd:annotation>
		            <xsd:documentation>
		            	Unik id for dette dokumentet i denne jobben.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:element name="fil" minOccurs="1" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
						Filen for dette dokumentet. Path relativ til roten av jobb-zip-arkivet
						1. Filstien kan bare inneholde norske tegn, tall, "-" eller "_"
						2. Filstien kan bare inneholde ett "." tegn, etterfulgt av extension
						3. Path-separator er "/". Merk at filstien ikke kan begynne med "/"

						For å unngå problemer knyttet til encoding av filnavn i Zip-arkivet,
						anbefaler vi å utelate bruk av særnorske tegn.
		            </xsd:documentation>
		        </xsd:annotation>
		        <xsd:simpleType>
		        	<xsd:restriction base="xsd:string">
		        		<xsd:pattern value="([a-zA-Z\d\-_æøåÆØÅ]+)(/[a-zA-Z\d\-_æøåÆØÅ]+)*(/|\.[a-zA-Z\d]+)"/>
		        	</xsd:restriction>
		        </xsd:simpleType>
			</xsd:element>
			<xsd:element name="innstillinger" type="dp:dokument-innstillinger" minOccurs="0" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		            	Innstillinger for dette dokumentet.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="faktura">
	    <xsd:complexContent>
	      <xsd:extension base="dp:dokument">
              <xsd:sequence>
                  <xsd:element name="kid" minOccurs="1" maxOccurs="1" nillable="false">
                      <xsd:simpleType>
                          <xsd:restriction base="xsd:string">
                              <xsd:pattern value="[0-9]{2,25}"/>
                          </xsd:restriction>
                      </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="beloep" minOccurs="1" maxOccurs="1" nillable="false">
                      <xsd:simpleType>
                          <xsd:restriction base="xsd:decimal">
                              <xsd:fractionDigits value="2"/>
                              <xsd:totalDigits value="15"/>
                          </xsd:restriction>
                      </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="kontonummer" minOccurs="1" maxOccurs="1" nillable="false">
                      <xsd:annotation>
                          <xsd:documentation>
                              Betalingsmottakers kontonummer. 11 siffer uten mellomrom eller punktum.
                          </xsd:documentation>
                      </xsd:annotation>
                      <xsd:simpleType>
                          <xsd:restriction base="xsd:string">
                              <xsd:pattern value="[0-9]{11}"/>
                          </xsd:restriction>
                      </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="forfallsdato" type="xsd:date" minOccurs="1" maxOccurs="1" nillable="false" />
              </xsd:sequence>
	      </xsd:extension>
	    </xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="brev">
        <xsd:annotation>
            <xsd:documentation>
            	Et brev er en kobling mellom en mottaker et dokument og evt. vedlegg (dokumenter)
            </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="mottaker" type="dp:mottaker" minOccurs="1" maxOccurs="1"/>
    		<xsd:element name="innstillinger" minOccurs="0" maxOccurs="1" type="dp:brev-innstillinger"/>
			<xsd:element name="dokument" minOccurs="1" maxOccurs="1" type="xsd:IDREF" >
				<xsd:annotation>
		            <xsd:documentation>
		            	Id på hoveddokumentet som skal sendes til denne mottakeren.
		            </xsd:documentation>
					<xsd:appinfo>
						<jaxb:property>
							<jaxb:baseType name="XmlDokument" />
						</jaxb:property>
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="vedlegg" minOccurs="0" maxOccurs="1" type="dp:dokument-ref-liste"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="afp-mottaker-identifikasjon">
        <xsd:annotation>
            <xsd:documentation>
            	Brukes til å fortelle Digipost hvilke TLE-felter i AFP-filen som skal brukes til mottaker-identifikasjon.
            </xsd:documentation>
        </xsd:annotation>
		<xsd:sequence>
			<xsd:element name="kunde-id" type="xsd:string" minOccurs="1" maxOccurs="1">
		        <xsd:annotation>
		            <xsd:documentation>
		            	Navnet på TLE-feltet som inneholder avsenders egne unike id for postmottakeren.
		            </xsd:documentation>
		        </xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="1" maxOccurs="1">
				<xsd:element name="foedselsnummer" type="xsd:string" minOccurs="1" maxOccurs="1">
			        <xsd:annotation>
			            <xsd:documentation>
			            	Navnet på TLE-feltet som inneholder mottakers fødselsnummer.
			            </xsd:documentation>
			        </xsd:annotation>
			    </xsd:element>
				<xsd:element name="digipostadresse" type="xsd:string" minOccurs="1" maxOccurs="1">
			        <xsd:annotation>
			            <xsd:documentation>
			            	Navnet på TLE-feltet som inneholder mottakers Digipostadresse.
			            </xsd:documentation>
			        </xsd:annotation>
			    </xsd:element>

			    <xsd:sequence>
		            <xsd:element name="navn" type="dp:afp-navn" minOccurs="1" maxOccurs="1" />
					<xsd:element name="adresse" type="dp:afp-adresse" minOccurs="1" maxOccurs="1" />
					<xsd:element name="foedselsdato" type="xsd:string" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>

			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="autentiseringsnivaa">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:typesafeEnumClass />
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PASSORD">
				<xsd:annotation>
					<xsd:documentation>Standardvalg. Krever kun passord av mottaker for å lese.</xsd:documentation>
					<xsd:appinfo>
						<jaxb:typesafeEnumMember name="PASSORD" />
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TO_FAKTOR">
				<xsd:annotation>
					<xsd:documentation>Krever to-faktor-autentisering av mottaker for å lese.</xsd:documentation>
					<xsd:appinfo>
						<jaxb:typesafeEnumMember name="TO_FAKTOR" />
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="sensitivitetsnivaa">
		<xsd:annotation>
			<xsd:appinfo>
				<jaxb:typesafeEnumClass />
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NORMALT">
				<xsd:annotation>
					<xsd:documentation>Standardvalg. Brukes for ikke-sensitive meldinger. Metadata om meldingen, 
					slik som avsender og emne, vil vises i varsler som sendes ut (epost-varsel og evt. sms-varsel).
					Metadata om meldingen vil også vises selv om brukeren ikke er innlogget på et sikkerhetsnivå
					som gjør at innholdet i meldingen kan vises (styrt av "autentiseringsnivaa")</xsd:documentation>
					<xsd:appinfo>
						<jaxb:typesafeEnumMember name="NORMALT" />
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SENSITIVT">
				<xsd:annotation>
					<xsd:documentation>Brukes for sensitive meldinger. Metadata om meldingen, 
					slik som avsender og emne, vil skjules i varsler som sendes ut (epost-varsel og evt. sms-varsel).
					Metadata om meldingen vil også skjules når brukeren er innlogget på et sikkerhetsnivå
					som gjør at innholdet i meldingen ikke kan vises (styrt av "autentiseringsnivaa")</xsd:documentation>
					<xsd:appinfo>
						<jaxb:typesafeEnumMember name="SENSITIVT" />
					</xsd:appinfo>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	
</xsd:schema>